	TextLabel.BackgroundTransparency = 1.000
	TextLabel.Position = UDim2.new(0.0190476216, 0, 0.166666687, 0)
	TextLabel.Size = UDim2.new(0.95873028, 0, 0.633333325, 0)
	TextLabel.Font = Enum.Font.SourceSans
