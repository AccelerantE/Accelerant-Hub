--||Source||--

for i, v in pairs(LocalMods:GetChildren()) do
	if v:FindFirstChild("Bar") then
		local Fire = false
		local Bar = v.Bar
		local player = game.Players.LocalPlayer
		local Mouse = player:GetMouse()
		local UserInputService = game:GetService("UserInputService")
		local ap = Vector2.new(v.AbsolutePosition.X, v.AbsolutePosition.Y)
		local as = Vector2.new(v.AbsoluteSize.X, v.AbsoluteSize.Y)
		local x = Mouse.X
		local down = false

		v.MouseButton1Down:Connect(function()
			Bar.Size = UDim2.new(0, (Mouse.X - ap.X), 1, 0)
			Fire = true
			down = true
		end)

		UserInputService.InputEnded:Connect(function(input, gp)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				down = false
				Fire = false
			end
		end)

		Mouse.Move:Connect(function()
			if down == true then
				Fire = false
				Fire = true

				if Mouse.X < ap.X then
					Bar.Size = UDim2.new(0, 0, 1, 0)
				elseif Mouse.X > (ap.X + as.X) then
					Bar.Size = UDim2.new(0, as.X, 1, 0)
				else
					Bar.Size = UDim2.new(0, (Mouse.X - ap.X), 1, 0)
				end
				if v.Name == "Speed" then
					Speed = 1000 * (Bar.Size.X.Offset / as.X)
					if aS == true then
						Player.Character.Humanoid.WalkSpeed = Speed
					end
					v.CurrentValue.Text = t(math.floor(Speed))
				elseif v.Name == "Jump" then
					Jump = 300 * (Bar.Size.X.Offset / as.X)
					if aj == true then
						if Player.Character.Humanoid.RigType == Enum.HumanoidRigType.R6 then
							Player.Character.Humanoid.JumpPower = Jump
						elseif Player.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
							Player.Character.Humanoid.JumpHeight = Jump
						end
					end
					v.CurrentValue.Text = t(math.floor(Jump))
				end
			end
		end)
	end
end

LocalMods.SetSpeed.MouseButton1Click:Connect(function()
	if aS == false then
		aS = true
		LocalMods.SetSpeed.TextLabel.TextColor3 = Color3.fromRGB(30, 30, 30)
		LocalMods.SetSpeed.BackgroundColor3 = Color3.fromRGB(120, 50, 255)
		befS = Player.Character.Humanoid.WalkSpeed
		Player.Character.Humanoid.WalkSpeed = Speed
	else
		
		aS = false
		LocalMods.SetSpeed.TextLabel.TextColor3 = Color3.fromRGB(120, 50, 255)
		LocalMods.SetSpeed.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
		Player.Character.Humanoid.WalkSpeed = befS
	end
end)

LocalMods.SetJump.MouseButton1Click:Connect(function()
	if aj == false then
		aj = true
		LocalMods.SetJump.TextLabel.TextColor3 = Color3.fromRGB(30, 30, 30)
		LocalMods.SetJump.BackgroundColor3 = Color3.fromRGB(255, 100, 50)
		if Player.Character.Humanoid.RigType == Enum.HumanoidRigType.R6 then
			befJ = Player.Character.Humanoid.JumpPower
			Player.Character.Humanoid.JumpPower = Jump
		elseif Player.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
			befJ = Player.Character.Humanoid.JumpHeight
			Player.Character.Humanoid.JumpHeight = Jump
		end
	else
		LocalMods.SetJump.TextLabel.TextColor3 = Color3.fromRGB(255, 100, 50)
		LocalMods.SetJump.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
		aj = false
		if Player.Character.Humanoid.RigType == Enum.HumanoidRigType.R6 then
			Player.Character.Humanoid.JumpPower = befJ
		elseif Player.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
			Player.Character.Humanoid.JumpHeight = befJ
		end
	end
end)

for i, v in sS:GetChildren() do
	local thisChecker = Checker - 1
	if v:IsA("ImageButton") then
		v.MouseButton1Click:Connect(function()
			if thisChecker ~= Checker - 1 then return end
			loadstring(Scripts[v.Name])()
		end)
	end
end

