	_ESP(v)
	_ESP(v)

	v.CharacterAdded:Connect(function()
		repeat task.wait() until v.Character ~= nil
		repeat task.wait() until v.Character:FindFirstChild("HumanoidRootPart") ~= nil
		_ESP(v)
		_ESP(v)
	end)
end

Players.PlayerAdded:Connect(function(v)
	repeat task.wait() until v.Character ~= nil
	repeat task.wait() until v.Character:FindFirstChild("HumanoidRootPart") ~= nil
	_ESP(v)
	_ESP(v)

	v.CharacterAdded:Connect(function()
		repeat task.wait() until v.Character ~= nil
		repeat task.wait() until v.Character:FindFirstChild("HumanoidRootPart") ~= nil
		_ESP(v)
		_ESP(v)
	end)
end)

return true
