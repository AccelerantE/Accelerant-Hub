	for i, v in sS:GetChildren() do
		local thisChecker = Checker - 1
		if v:IsA("ImageButton") then
			v.MouseButton1Click:Connect(function()
				if thisChecker ~= Checker - 1 then return end
				loadstring(Scripts[v.Name])()
			end)
		end
	end
end)

