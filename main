		AddScript.sSource.Text = Content
	end
end)

AddScript.ColorPicker.MouseButton1Click:Connect(function()
	AddScript.Selector.Visible = not AddScript.Selector.Visible
end)

coroutine.wrap(function()
	local rgb = Selector.RGB
	local value = Selector.Value
	local preview = Selector.Preview

	local selectedColor = Color3.fromHSV(1,1,1)
	local colorData = {1,1,1}

	local mouse1down = false

	local function setColor(hue,sat,val)
		colorData = {hue or colorData[1],sat or colorData[2],val or colorData[3]}
		selectedColor = Color3.fromHSV(colorData[1],colorData[2],colorData[3])
		local r,g,b = math.floor((selectedColor.R*255)+0.5),math.floor((selectedColor.G*255)+0.5),math.floor((selectedColor.B*255)+0.5)
		AddScript.sColor.Text = r..", "..g..", "..b
		preview.BackgroundColor3 = selectedColor
		value.ImageColor3 = Color3.fromHSV(colorData[1],colorData[2],1)
	end

	local function inBounds(frame)
		local x,y = Mouse.X - frame.AbsolutePosition.X,Mouse.Y - frame.AbsolutePosition.Y
		local maxX,maxY = frame.AbsoluteSize.X,frame.AbsoluteSize.Y
		if x >= 0 and y >= 0 and x <= maxX and y <= maxY then
			return x/maxX,y/maxY
		end
	end

	local function updateRGB()
		if mouse1Down == true then
			local x,y = inBounds(rgb)
			if x and y then
				rgb:WaitForChild("Marker").Position = UDim2.new(x,0,y,0)
				setColor(1 - x,1 - y)
			end

			local x,y = inBounds(value)
			if x and y then
				value:WaitForChild("Marker").Position = UDim2.new(0.5,0,y,0)
				setColor(nil,nil,1 - y)
			end
		end
	end

	Mouse.Move:connect(updateRGB)

	Mouse.Button1Down:connect(function()mouse1Down = true end)
	Mouse.Button1Up:connect(function()mouse1Down = false end)
end)()

AddScript.Confirm.MouseButton1Click:Connect(function()
	local RGB = string.gsub(AddScript.sColor.Text, " ", "")
	RGB = string.split(RGB, ",")
	local R = tonumber(RGB[1])
	local G = tonumber(RGB[2])
	local B = tonumber(RGB[3])
	RGB = Color3.fromRGB(R, G, B)
	Scripts[AddScript.sName.Text] = AddScript.sSource.Text
	Checker += 1
	
	-- Instances:

