	UICorner.Parent = NewScript

	UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, RGB), ColorSequenceKeypoint.new(0.73, Color3.fromRGB(35, 31, 31)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(30, 30, 30))}
	UIGradient.Parent = NewScript

