--||Functions||--

local function TweenFromLeft(Frame: Frame?, Time: number, Position: UDim2)
	local p = Frame.Position
	Frame.Position = Position - UDim2.new(1, 0, 0, 0)
	Frame.Visible = true
	TweenService:Create(
		Frame,
		TweenInfo.new(Time, Enum.EasingStyle.Quad),
		{Position = Position}
	):Play()
end

local function TweenFromRight(Frame: Frame?, Time: number, Position: UDim2)
	local p = Frame.Position
	Frame.Position = Position + UDim2.new(1, 0, 0, 0)
	Frame.Visible = true
	TweenService:Create(
		Frame,
		TweenInfo.new(Time, Enum.EasingStyle.Quad),
		{Position = Position}
	):Play()
end

local function TweenToPosition(Frame: Frame?, Time: number, Position: UDim2)
	local p = Frame.Position
	TweenService:Create(
		Frame,
		TweenInfo.new(Time, Enum.EasingStyle.Quad, Enum.EasingDirection.In),
		{Position = Position}
	):Play()
	task.delay(Time, function()
		Frame.Position = p
		Frame.Visible = false
	end)
end

local function TweenFromBottom(Frame: Frame?, Time: number, Position: UDim2)
	local p = Frame.Position
	Frame.Position = Position - UDim2.new(0, 0, -1, 0)
	Frame.Visible = true
	TweenService:Create(
		Frame,
		TweenInfo.new(Time, Enum.EasingStyle.Quad),
		{Position = Position}
	):Play()
end

local function GetServerInfo()
	ServerI.PiS.Text = "Players in server: "..t(#Players:GetChildren()).."/"..t(Players.MaxPlayers)
	local FriendsCount = 0
	for i, v in Players:GetPlayers() do
		if Player:IsFriendsWith(v.UserId) then
			FriendsCount += 1
		end
	end
	ServerI.FiS.Text = "Friends in server: "..t(FriendsCount)
end

local function ImageAnim(Frame)
	for i, v in Frame:GetChildren() do
		if v:IsA("ImageLabel") or v:IsA("ImageButton") then
			v.MouseEnter:Connect(function()
				TweenService:Create(
					v,
					TweenInfo.new(0.5, Enum.EasingStyle.Quad),
					{TileSize = UDim2.new(1,0, 1,0)}
				):Play()
			end)
			v.MouseLeave:Connect(function()
				TweenService:Create(
					v,
					TweenInfo.new(0.5, Enum.EasingStyle.Quad),
					{TileSize = UDim2.new(1.1,0, 1.1,0)}
				):Play()
			end)
		end
	end
end

local function TweenTransparency(Frame, t)
	TweenService:Create(
		Frame,
		TweenInfo.new(.5),
		{Transparency = t}
	):Play()
end

local function TweenImgTransparency(Frame, t)
	TweenService:Create(
		Frame,
		TweenInfo.new(.5),
		{ImageTransparency = t}
	):Play()
end

