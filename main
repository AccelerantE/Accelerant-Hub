RunService.RenderStepped:Connect(function()
	local p = t(Player:GetNetworkPing())
	p = string.split(p, "")
	if p[2] then
		ServerI.Ping.Text = "Ping: "..p[1]..p[2]..p[3]
	end
	if p[4] then
		ServerI.Ping.Text = "Ping: "..p[1]..p[2]..p[3]..p[4]
	end
	if p[5] then
		ServerI.Ping.Text = "Ping: "..p[1]..p[2]..p[3]..p[4]..p[5]
	end
end)

--||Animations||--

coroutine.wrap(function()
	while task.wait(1) do
		if OffTiming >= 3 then
			for i, v in wTop:GetChildren() do
				if v:IsA("ImageButton") then
					TweenImgTransparency(v, .5)
				end
			end

			for i, v in wBottom:GetChildren() do
				if v:IsA("ImageButton") then
					TweenImgTransparency(v, .5)
				end
			end
			
			TweenTransparency(WorkBar, .5)
		end
		OffTiming += 1
	end
end)()

UserInputService.InputBegan:Connect(function(i)
	if i.KeyCode == Enum.KeyCode.K then
		if CurrentlyAnimating.WorkBar == true then return end
		if WorkBar.Visible == true then
			CurrentlyAnimating.WorkBar = true
			TweenToPosition(WorkBar, 0.2, UDim2.new(1.952, 0,0.289, 0))
			task.delay(.1, function()
				CurrentlyAnimating.WorkBar = false
			end)
		else
			CurrentlyAnimating.WorkBar = true
			TweenFromRight(WorkBar, 0.2, UDim2.new(0.952, 0,0.289, 0))
			task.delay(.1, function()
				CurrentlyAnimating.WorkBar = false
			end)
		end
	end
end)

for i, v in wTop:GetChildren() do
	if v:IsA("ImageButton") then
		v.MouseEnter:Connect(function()
			if v.ImageColor3 == hgColor then return end
			v.Size = UDim2.new(0.583, 0, 0.177, 0)
		end)
		v.MouseLeave:Connect(function()
			if v.Name == "Home" and HomeOpenned == true then return end
			if v.ImageColor3 == hgColor then return end
			v.Size = UDim2.new(0.483, 0,0.142, 0)
		end)
	end
end

for i, v in wBottom:GetChildren() do
