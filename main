task.spawn(function()
	local ExecutorName = identifyexecutor()

	for i, v in pairs(Banners) do
		if string.find(string.lower(ExecutorName), v["string"]) then
			hFolder.Executor.Image = v["Banner"]
			hFolder.Executor.exeName.Text = v["Text"]
			hFolder.Executor.SF.Text = "Special Functions: "..t(v["SF"])
		end
	end
end)

local Info = MarketPlace:GetProductInfo(game.PlaceId)
GameInfo.GameName.Text = Info.Name
GameInfo.GameIcon.Image = "rbxthumb://type=Asset&id="..Info.IconImageAssetId.."&w=150&h=150"
if game.CreatorType == Enum.CreatorType.User then
	local Succes, ret = pcall(function()
		return UserSerice:GetUserInfosByUserIdsAsync({game.CreatorId})
	end)

	if Succes then
		for i, User in pairs(ret) do
			GameInfo.GameIcon.GameOwner.Text = "By: "..User.Username
		end
	end
elseif game.CreatorType == Enum.CreatorType.Group then
	local Group = GroupService:GetGroupInfoAsync(game.CreatorId)
	GameInfo.GameIcon.GameOwner.Text = Group.Name
end
