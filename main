	Close.AutoButtonColor = false
	Close.Image = "rbxassetid://11597302926"
	Close.ImageTransparency = 0.100

	UICorner_2.CornerRadius = UDim.new(0.300000012, 0)
	UICorner_2.Parent = Close
	
	Close.MouseButton1Click:Connect(function()
		NewScript:Destroy()
	end)
end)

for i, v in pairs(Settings.WB:GetChildren()) do
	if v:IsA("TextLabel") and v:FindFirstChild("Check") then
		v.Check.MouseButton1Click:Connect(function()
			if v.Check.V.Visible == true then
				v.Check.V.Visible = false
				local WBB = string.gsub(v.Text, "Show ", "")
				wTop[WBB].Visible = false
			else
				v.Check.V.Visible = true
				local WBB = string.gsub(v.Text, "Show ", "")
				wTop[WBB].Visible = true
			end
		end)
	end
end

coroutine.wrap(function()
	local v = Settings.AB.FOV
	local Fire = false
	local Bar = v.Bar
	local player = game.Players.LocalPlayer
	local Mouse = player:GetMouse()
	local UserInputService = game:GetService("UserInputService")
	local ap = Vector2.new(v.AbsolutePosition.X, v.AbsolutePosition.Y)
	local as = Vector2.new(v.AbsoluteSize.X, v.AbsoluteSize.Y)
	local x = Mouse.X
	local down = false

	v.MouseButton1Down:Connect(function()
		Bar.Size = UDim2.new(0, (Mouse.X - ap.X), 1, 0)
		Fire = true
		down = true
	end)

	UserInputService.InputEnded:Connect(function(input, gp)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			down = false
			Fire = false
		end
	end)

	Mouse.Move:Connect(function()
		if down == true then
			Fire = false
			Fire = true

			if Mouse.X < ap.X then
				Bar.Size = UDim2.new(0, 0, 1, 0)
			elseif Mouse.X > (ap.X + as.X) then
				Bar.Size = UDim2.new(0, as.X, 1, 0)
			else
				Bar.Size = UDim2.new(0, (Mouse.X - ap.X), 1, 0)
			end
			FOV = 800 * (Bar.Size.X.Offset / as.X)
			v.CurrentValue.Text = t(math.floor(Speed))
		end
	end)
end)()

for i, v in pairs(Settings:GetChildren()) do
	if v:IsA("ScrollingFrame") then
		for x, y in pairs(v:GetChildren()) do
			if y ~= nil and y:IsA("TextButton") then
				y.MouseButton1Click:Connect(function()
					Settings.CurrentlyChangingColor.Text = y.Text
				end)
			end
		end
	end
end

coroutine.wrap(function()
	local rgb = Settings.Selector.RGB
	local value = Settings.Selector.Value
	local preview = Settings.Selector.Preview

	local selectedColor = Color3.fromHSV(1,1,1)
	local colorData = {1,1,1}

	local mouse1down = false

	local function setColor(hue,sat,val)
		colorData = {hue or colorData[1],sat or colorData[2],val or colorData[3]}
		selectedColor = Color3.fromHSV(colorData[1],colorData[2],colorData[3])
		local r,g,b = math.floor((selectedColor.R*255)+0.5),math.floor((selectedColor.G*255)+0.5),math.floor((selectedColor.B*255)+0.5)
		Settings.Selector.sColor.Text = r..", "..g..", "..b
		preview.BackgroundColor3 = selectedColor
		value.ImageColor3 = Color3.fromHSV(colorData[1],colorData[2],1)
		local RGB = string.gsub(Settings.Selector.sColor.Text, " ", "")
		RGB = string.split(RGB, ",")
		local R = tonumber(RGB[1])
		local G = tonumber(RGB[2])
		local B = tonumber(RGB[3])
		RGB = Color3.fromRGB(R, G, B)
		if Settings.CurrentlyChangingColor.Text == "Shoot-able color" then
			enSA = RGB
		elseif Settings.CurrentlyChangingColor.Text == "no Shoot-able color" then
			nonSA = RGB
		elseif Settings.CurrentlyChangingColor.Text == "FOV Color" then
			fColor = RGB
		elseif Settings.CurrentlyChangingColor.Text == "Highlight Color" then
			hgColor = RGB
			wBottom.Settings.ImageColor3 = hgColor
		end
	end

	local function inBounds(frame)
		local x,y = Mouse.X - frame.AbsolutePosition.X,Mouse.Y - frame.AbsolutePosition.Y
		local maxX,maxY = frame.AbsoluteSize.X,frame.AbsoluteSize.Y
		if x >= 0 and y >= 0 and x <= maxX and y <= maxY then
			return x/maxX,y/maxY
		end
	end

	local function updateRGB()
		if mouse1Down == true then
			local x,y = inBounds(rgb)
			if x and y then
				rgb:WaitForChild("Marker").Position = UDim2.new(x,0,y,0)
				setColor(1 - x,1 - y)
			end

			local x,y = inBounds(value)
			if x and y then
				value:WaitForChild("Marker").Position = UDim2.new(0.5,0,y,0)
				setColor(nil,nil,1 - y)
			end
		end
	end

	Mouse.Move:connect(updateRGB)

	Mouse.Button1Down:connect(function()mouse1Down = true end)
	Mouse.Button1Up:connect(function()mouse1Down = false end)
end)()

--||Connections-Functions||--

--ESP--

local function BoxESP(v: Player)
	if v ~= Player and v.Character ~= nil and v.Character:FindFirstChild("HumanoidRootPart") ~= nil and v.Character:FindFirstChild("Humanoid") and v.Character.Humanoid.Health > 0 then
		local HeadOffset = Vector3.new(0, 0.5, 0)
		local LegOffset = Vector3.new(0, 3, 0)

		local Box = Drawing.new("Square")
		Box.Thickness = 1
		Box.Transparency = 1
		Box.Color = Color3.fromRGB(255, 255, 255)
		Box.Filled = false
		Box.Visible = false

		local function con()
			RunService.RenderStepped:Connect(function()
				if espS.ESP.Check.V.Visible == true then
					Box.Visible = true
					if espS.Box.Check.V.Visible == true and v.Character ~= nil and v.Character:FindFirstChild("HumanoidRootPart") ~= nil and v.Character:FindFirstChild("Humanoid") and v.Character.Humanoid.Health > 0 then
						local Vector, onScreen = Camera:worldToViewportPoint(v.Character.HumanoidRootPart.Position)

						local RootPosition, RootVisible = Camera:worldToViewportPoint(v.Character.HumanoidRootPart.Position)
						local HeadPosition = Camera:worldToViewportPoint(v.Character.Head.Position + HeadOffset)
						local LegPosition = Camera:worldToViewportPoint(v.Character.HumanoidRootPart.Position - LegOffset)

						if espS.Teamates.Check.V.Visible == true or Player.Team ~= v.Team then
							if onScreen then
								Box.Size = Vector2.new(1000/RootPosition.Z, HeadPosition.Y - LegPosition.Y)
								Box.Position = Vector2.new(RootPosition.X - Box.Size.X/2, RootPosition.Y - Box.Size.Y/2)
								local Check = Camera:GetPartsObscuringTarget({v.Character.Head}, Player.Character:GetChildren())
								if espS.wCheck.Check.V.Visible == true then
									local RayCastInformation = RaycastParams.new()
									RayCastInformation.FilterDescendantsInstances = {Player.Character}
									RayCastInformation.FilterType = Enum.RaycastFilterType.Blacklist
									local Ray = workspace:Raycast(Player.Character.Head.Position, (v.Character.Head.Position - Player.Character.Head.Position).Unit*1000, RayCastInformation)
									if v.Character:IsAncestorOf(Ray.Instance) then
										Box.Color = enSA
									else
										Box.Color = nonSA
									end
								else
									Box.Color = enSA
								end
								Box.Visible = true
							else
								Box.Visible = false
							end
						else
							Box.Visible = false
						end
					else
						Box.Visible = false
					end
				else
					Box.Visible = false
				end
			end)
		end
		coroutine.wrap(con)()
	end
end

local function _ESPNames(Bool)
	if Bool == true then
		for i, p in game.Players:GetPlayers() do
			if p ~= Player then
				local aName = p.Character.Head:FindFirstChild("OverHead")

				if aName then aName:Destroy() end

				if espS.Teamates.Check.V.Visible == true and p.Team ~= Player.Team then continue end
				if espS.Enemies.Check.V.Visible == true and p.Team == Player.Team then continue end

				local OverHead = Instance.new("BillboardGui")
				local Role = Instance.new("TextLabel")

				--Properties:

				OverHead.Name = "OverHead"
				OverHead.Parent = p.Character.Head
				OverHead.LightInfluence = 1.000
				OverHead.Size = UDim2.new(8, 0, 1, 0)
				OverHead.StudsOffset = Vector3.new(0, 3, 0)

				Role.Name = "Name"
				Role.Parent = OverHead
				Role.BackgroundColor3 = Color3.fromRGB(0, 255, 64)
				Role.BackgroundTransparency = 1.000
				Role.Position = UDim2.new(-0.25, 0, 0.899999976, 0)
				Role.Size = UDim2.new(1.5, 0, 0.699999988, 0)
				Role.Font = Enum.Font.Nunito
				Role.Text = p.Name
				Role.TextColor3 = p.TeamColor.Color
				Role.TextScaled = true
				Role.TextSize = 10.000
				Role.TextWrapped = true
			end
		end
	elseif Bool == false or espS.ESPNames.Check.V.Visible == false then
		for i, p in game.Players:GetPlayers() do
			if p ~= Player then
				local aName = p.Character.Head:FindFirstChild("OverHead")

				if aName then aName:Destroy() end
			end
		end
	end
end

local function TracersESP(v: Player)
	if espS.Teamates.Check.V.Visible == false or Player.Team ~= v.Team then
		if v ~= Player and v.Character ~= nil and v.Character:FindFirstChild("HumanoidRootPart") ~= nil and v.Character:FindFirstChild("Humanoid") and v.Character.Humanoid.Health > 0 then
			local Line = Drawing.new("Line")
			Line.Thickness = 1
			Line.Color = Color3.fromRGB(255, 255, 255)
			Line.Transparency = 1
			Line.Visible = false

			local function con()
				RunService.RenderStepped:Connect(function()
					if espS.ESP.Check.V.Visible == true then
						Line.Visible = true
						if espS.Teamates.Check.V.Visible == true or Player.Team ~= v.Team then
							if v.Character ~= nil and v.Character:FindFirstChild("HumanoidRootPart") ~= nil and v.Character:FindFirstChild("Humanoid") and v.Character.Humanoid.Health > 0 then
								local Vector, onScreen = Camera:worldToViewportPoint(v.Character.HumanoidRootPart.Position)
								Line.From = Vector2.new(Camera.ViewportSize.X/2, Camera.ViewportSize.Y/1.2)

								if onScreen == true then
									Line.To = Vector2.new(Vector.X, Vector.Y)
									local Check = Camera:GetPartsObscuringTarget({v.Character.Head}, Player.Character:GetChildren())
									if espS.wCheck.Check.V.Visible == true then
										local RayCastInformation = RaycastParams.new()
										RayCastInformation.FilterDescendantsInstances = {Player.Character}
										RayCastInformation.FilterType = Enum.RaycastFilterType.Blacklist
										local Ray = workspace:Raycast(Player.Character.Head.Position, (v.Character.Head.Position - Player.Character.Head.Position).Unit*1000, RayCastInformation)
										if v.Character:IsAncestorOf(Ray.Instance) then
											Line.Color = enSA
										else
											Line.Color = nonSA
										end
									else
										Line.Color = enSA
									end
									Line.Visible = true
								else
									Line.Visible = false
								end
							else
								Line.Visible = false
							end
						else
							Line.Visible = false
						end
					else
						Line.Visible = false
					end
				end)
			end
			coroutine.wrap(con)()
		end
	end
end

local function _ESP(v)
	TracersESP(v)
	BoxESP(v)
end

--Aimbot--

local function Aimbot()
	local Targeting = false
	local Down = false
	local Target
	local Circle = Drawing.new("Circle")
	Circle.Radius = FOV
	Circle.Color = fColor
	Circle.Position = Vector2.new(Camera.ViewportSize.X/2, Camera.ViewportSize.Y/2)
	Circle.Transparency = 1
	Circle.Thickness = 1
	Circle.Visible = false

        --[[game:GetService("UserInputService").InputBegan:Connect(function(i)
            if i.KeyCode == Enum.KeyCode.E then
                Down = true
                Target = nil
                Targeting = false
            end
        end)

        game:GetService("UserInputService").InputEnded:Connect(function(i)
            if i.KeyCode == Enum.KeyCode.E then
                Down = false
                Target = nil
                Targeting = false
            end
        end)]]

	Mouse.Button2Down:Connect(function()
		Down = true
		Target = nil
		Targeting = false
	end)

	Mouse.Button2Up:Connect(function()
		Down = false
		Target = nil
		Targeting = false
	end)

	local RayCastInformation = RaycastParams.new()
	RayCastInformation.FilterDescendantsInstances = {Player.Character}
	RayCastInformation.FilterType = Enum.RaycastFilterType.Blacklist

	coroutine.wrap(function()
		RunService.RenderStepped:Connect(function(nect)
			if aimS.Aimbot.Check.V.Visible == true then
				local mpv2 = Vector2.new(Mouse.X, Mouse.Y/0.93)
				if aimS.ShowFov.Check.V.Visible == true then
					Circle.Visible = true
					Circle.Radius = FOV
					Circle.Color = fColor
					Circle.Position = mpv2
				else
					Circle.Visible = false
				end
				if Down == true then
					print("down")
					if Targeting == false then
						for _, p in pairs(Players:GetPlayers()) do
							if p ~= Player and p.Character ~= nil and p.Character:FindFirstChild("HumanoidRootPart") ~= nil and p.Character:FindFirstChild("Humanoid") and p.Character.Humanoid.Health > 0 then
								local pos, vis = Camera:WorldToViewportPoint(p.Character.Head.Position)
								local dis = math.huge
								pos = Vector2.new(pos.X, pos.Y)

								if (pos - mpv2).Magnitude < dis then
									if aimS.useFov.Check.V.Visible == false or (pos - mpv2).Magnitude <= FOV then
										print("Pass 1")
										if aimS.ScreenCheck.Check.V.Visible == false or vis then
											print("Pass 2")
											if aimS.TeamCheck.Check.V.Visible == false or Player.Team ~= p.Team then
												print("Pass 3")
												local Ray = workspace:Raycast(Player.Character.Head.Position, (p.Character.Head.Position - Player.Character.Head.Position).Unit*1000, RayCastInformation)

												if p.Character:IsAncestorOf(Ray.Instance) then
													print(p)
													dis = pos - mpv2
													Target = p
												end
											end
										end
									end
								end
							end
						end
					end
					if Target ~= nil then
						local Ray = workspace:Raycast(Player.Character.Head.Position, (Target.Character.Head.Position - Player.Character.Head.Position).Unit*1000, RayCastInformation)
						if Target.Character ~= nil and Target.Character:FindFirstChild("HumanoidRootPart") and Target.Character:FindFirstChild("Humanoid") and Target.Character.Humanoid.Health > 0 and Target.Character:IsAncestorOf(Ray.Instance) then
							print("Moving")
							Camera.CFrame = CFrame.new(Camera.CFrame.Position, Target.Character.Head.CFrame.Position)
	                        --[[local pos = Camera:worldToViewportPoint(Target.Character.Head.Position)
	                        mousemoveabs(pos.X, pos.Y)
	                        Targeting = true]]
						else
							print("Fall")
							Targeting = false
							Target = nil
							Down = false
						end
					end
				end
			end
		end)
	end)()
end

local function OpenHome()
	CurrentlyAnimating.Home = true
	print(hgColor)
	wTop.Home.ImageColor3 = hgColor
	wTop.Home.Size = UDim2.new(0.583, 0, 0.177, 0)
	local Blur = Instance.new("BlurEffect")
	Blur.Parent = game:GetService("Lighting")
	Blur.Name = "AccelerantBlur"
	Blur.Size = 0
	TweenService:Create(Blur, TweenInfo.new(0.1), {Size = 15}):Play()
	TweenFromLeft(hFolder.aclrInfo, .1, UDim2.new(0.012, 0,0.021, 0))
	task.delay(0.03, function()
		TweenFromLeft(hFolder.GameInfo, .1, UDim2.new(0.011, 0, 0.193, 0))
	end)
	task.delay(0.03, function()
		TweenFromLeft(hFolder.Executor, .1, UDim2.new(0.011, 0, 0.63, 0))
	end)
	task.delay(0.03, function()
		TweenFromLeft(hFolder.ServerInfo, .1, UDim2.new(0.177, 0, 0.184, 0))
	end)
	task.delay(0.03, function()
		TweenFromLeft(hFolder.Friends, .1, UDim2.new(0.176, 0, 0.357, 0))
	end)
	task.delay(.11, function()
		CurrentlyAnimating.Home = false
		HomeOpenned = true
	end)
end

local function CloseHome()
	CurrentlyAnimating.Home = true
	wTop.Home.ImageColor3 = Color3.fromRGB(255, 255, 255)
	wTop.Home.Size = UDim2.new(0.483, 0,0.142, 0)
	local Blur = game:GetService("Lighting"):FindFirstChild("AccelerantBlur")
	if Blur then
		TweenService:Create(Blur, TweenInfo.new(0.1), {Size = 0}):Play()
		task.delay(.1, function()
			Blur:Destroy()
		end)
	end
	TweenToPosition(hFolder.Friends, .1, UDim2.new(-1.176, 0, 0.357, 0))
	task.delay(0.03, function()
		TweenToPosition(hFolder.ServerInfo, .1, UDim2.new(-1.177, 0, 0.184, 0))
	end)
	task.delay(0.03, function()
		TweenToPosition(hFolder.Executor, .1, UDim2.new(-1.011, 0, 0.63, 0))
	end)
	task.delay(0.03, function()
		TweenToPosition(hFolder.GameInfo, .1, UDim2.new(-1.011, 0, 0.193, 0))
	end)
	task.delay(0.03, function()
		TweenToPosition(hFolder.aclrInfo, .1, UDim2.new(-1.012, 0,0.021, 0))
	end)
	task.delay(.11, function()
		CurrentlyAnimating.Home = false
		HomeOpenned = false
	end)
end

