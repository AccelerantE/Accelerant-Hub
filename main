local function CloseAll(Except: string)
	if Except ~= "Local" then
		CloseLocal()
	end
	if Except ~= "Scripts" then
		CloseScripts()
	end
	if Except ~= "Home" then
		CloseHome()
	end
	if Except ~= "Combat" then
		CloseCombat()
	end
	if Except ~= "Settings" then
		CloseSettings()
	end
end

--||Connections||--

wBottom.Settings.MouseButton1Click:Connect(function()
	if Settings.Visible == false then
		if CurrentlyAnimating.Settings == true then return end
		CloseAll("Settings")
		OpenSettings()
	else
		if CurrentlyAnimating.Settings == true then return end
		CloseSettings()
	end
end)

wTop.Home.MouseButton1Click:Connect(function()
	if HomeOpenned == false then
		if CurrentlyAnimating.Home == true then return end
		CloseAll("Home")
		OpenHome()
	else
		if CurrentlyAnimating.Home == true then return end
		CloseHome()
	end
end)

wTop.Local.MouseButton1Click:Connect(function()
	if CurrentlyAnimating.Local == true then return end
	if Screen.LocalMods.Visible == false then
		CloseAll("Local")
		OpenLocal()
	else
		CloseLocal()
	end
end)

wTop.Scripts.MouseButton1Click:Connect(function()
	if CurrentlyAnimating.Scripts == true then return end
	if Screen.Scripts.Visible == false then
		CloseAll("Scripts")
		OpenScripts()
	else
		CloseScripts()
	end
end)

wTop.Combat.MouseButton1Click:Connect(function()
	if CombatOpenned == false then
		if CurrentlyAnimating.Combat == true then return end
		CombatOpenned = true
		CloseAll("Combat")
		OpenCombat()
	else
		if CurrentlyAnimating.Combat == true then return end
		CombatOpenned = false
		CloseCombat()
	end
end)

